Find First and Last Position

class Solution {
    public int[] searchRange(int[] nums, int target) {
        int start=0,s=0,end=nums.length-1,e=nums.length-1,ans=-1,a=-1;
        int []res=new int[2];
        while(start<=end){
            int mid=start+(end-start)/2;
            if(nums[mid]==target){
                ans=mid;
                end=mid-1;
            }
            else if(nums[mid]<target){
                start=mid+1;
            }
            else{
                end=mid-1;
            }
        }
            while(s<=e){
                int m=s+(e-s)/2;
                if(nums[m]==target){
                    a=m;
                    s=m+1;
                }
                else if(nums[m]>target){
                    e=m-1;
                }
                else{
                    s=m+1;
                }
            }
            
        
        res[1]=a;
        res[0]=ans;
        return res;
        
    }
}
